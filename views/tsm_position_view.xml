<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="tsm_position_form" model="ir.ui.view">
            <field name="name">tsm.position.form</field>
            <field name="model">tsm.position</field>
            <field name="arch" type="xml">
                <form string="Model Description">
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="tsm_position_tree" model="ir.ui.view">
            <field name="name">tsm.position.tree</field>
            <field name="model">tsm.position</field>
            <field name="arch" type="xml">
                <tree string="Model Description">
                    <field name="name" />
                </tree>
            </field>
        </record>


    </data>
</odoo>