<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="tsm_department_form" model="ir.ui.view">
            <field name="name">tsm.department.form</field>
            <field name="model">tsm.department</field>
            <field name="arch" type="xml">
                <form string="Model Description">
                    <sheet>
                        <group col="2">
                            <group>
                                <field name="name" />
                            </group>
                            <group>
                                <field name="head"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="tsm_department_tree" model="ir.ui.view">
            <field name="name">tsm.department.tree</field>
            <field name="model">tsm.department</field>
            <field name="arch" type="xml">
                <tree string="Model Description">
                    <field name="name" />
                    <field name="head" />
                </tree>
            </field>
        </record>


    </data>
</odoo>